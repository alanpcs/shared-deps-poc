{
  "name": "@alanpcs/shared-deps-poc",
  "version": "1.0.4",
  "description": "Template to create a package to be shared",
  "author": "AlanPCS",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/typings/index.d.ts",
  "files": [
    "dist",
    "dist/typings"
  ],
  "scripts": {
    "test": "mocha --timeout 60000 --exit -r ts-node/register src/**/*.test.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "build": "rm -rf dist && tsc",
    "deploy": "yarn build && npm publish"
  },
  "keywords": [
    "typescript",
    "github packages",
    "packages",
    "boilerplate"
  ],
  "resolutions": {
    "brace-expansion": "^2.0.2"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "chai": "^6.2.0",
    "eslint": "9.38.0",
    "eslint-plugin-jest": "^29.0.1",
    "jiti": "^2.6.1",
    "mocha": "^11.7.4",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "~5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/alanpcs/shared-deps-poc.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/alanpcs"
  }
}
